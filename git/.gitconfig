[alias]
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	unstage = reset HEAD --
	staged = diff --staged
	last = log -1 HEAD
	aware = add -N
	aliases = config --get-regexp '^alias\\.'
	yolo = push --force
	ta = log --all --graph --oneline --decorate --simplify-by-decoration --abbrev-commit
