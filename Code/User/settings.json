{
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    // "asvetliakov.vscode-neovim": 1
  },
  "search.collapseResults": "alwaysCollapse",
  // Font settings
  "editor.fontSize": 14.5,
  "editor.fontFamily": "'Monaspace Neon', monospace",
  "editor.fontLigatures": true,
  // Editor settings
  "editor.minimap.enabled": false,
  // Tab vs Spaces
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "editor.detectIndentation": true,
  // Whitespace etc
  "editor.renderWhitespace": "all",
  "editor.trimAutoWhitespace": true,
  "editor.stickyScroll.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.hover.sticky": true,
  "editor.lineNumbers": "relative",
  // folding
  "editor.folding": true,
  "editor.foldingHighlight": true,
  "editor.foldingImportsByDefault": false,
  "editor.foldingStrategy": "auto",
  // Go settings
  "gopls": {
    "formatting.local": "github.com/censys"
  },
  "go.useLanguageServer": true,
  "go.lintTool": "golint",
  "go.delveConfig": {},
  "go.toolsManagement.autoUpdate": true,
  // Debug settings
  "debug.openDebug": "openOnSessionStart",
  // language settings
  "[go]": {
    "editor.foldingImportsByDefault": false,
    "editor.formatOnSave": false,
    "editor.codeLens": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.formatting.provider": "black",
  // "python.linting.flake8Args": ["--ignore=W293"],
  // "python.linting.pylintEnabled": true,
  // "python.linting.pylintArgs": ["--enable=W0614"],
  "[python]": {
    "defaultInterpreterPath": "python3",
    "terminal.activateEnvInCurrentTerminal": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
    // "editor.showUnused": true,
    // Turn this off, as it keeps removing imports.
    // "source.organizeImports": "explicit",
    // "source.unusedImports": "explicit",
  },
  // "black-formatter.showNotification": "onWarning",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // "haskell.manageHLS": "GHCup",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "dockerfile": true,
    "go": true,
    "json": true,
    "jsonc": true,
    "makefile": true,
    "shellscript": true,
    "sql": true,
    "typescript": true,
    "python": true
  },
  "github.copilot.advanced": {},
  // "vs-kubernetes": {
  //   "vs-kubernetes.crd-code-completion": "disabled",
  //   "vscode-kubernetes.helm-path.linux": "$HOME/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
  // },
  "protoc": {
    "options": ["--proto_path=$HOME/dev/pipeline/proto"]
  },
  "workbench.startupEditor": "readme",
  "workbench.colorTheme": "Default High Contrast",
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "editorWhitespace.foreground": "#555"
    }
  },
  "explorer.autoReveal": false,
  "window.restoreFullscreen": true,
  "window.density.editorTabHeight": "compact",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.splitInGroupLayout": "horizontal",
  "workbench.editor.splitSizing": "distribute",
  "diffEditor.codeLens": true,
  "git.openRepositoryInParentFolders": "never",
  // "workbench.experimental.settingsProfiles.enabled": true
  // efficiency improvements
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true,
    "**/tmp/**": true,
    "**/dist/**": true,
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/node_modules": true,
    "venv": true,
    "*.sublime-*": true,
    "env*": true,
    "**/tmp/**": true,
    "**/node_modules/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/dist/**": true
  },
  "telemetry.telemetryLevel": "crash",
  "extensions.autoUpdate": false,
  "search.followSymlinks": false,
  "workbench.reduceMotion": "on",
  "extensions.ignoreRecommendations": true,
  "githubPullRequests.pullBranch": "never",
  "window.customTitleBarVisibility": "auto",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "redhat.telemetry.enabled": false,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.history": 500,
  "vim.smartRelativeLine": true,
  "vim.showMarksInGutter": true,
  "vim.autoindent": false,
  "vim.easymotionDimBackground": false,
  "vim.joinspaces": false,
  "vim.startofline": false,
  "vim.targets.smartQuotes.breakThroughLines": false,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME/.vimrc",
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>","<leader>"],
      "commands": ["whichkey.show"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>","<leader>"],
      "commands": ["whichkey.show"]
    },
  ],
  "whichkey.bindingOverrides": [
    {
      "keys": "d",
      "name": "Debugging",
      "type": "bindings",
      "bindings": [
        {
          "key": "b",
          "name": "Set breakpoint",
          "type": "command",
          "command": "editor.debug.action.toggleBreakpoint",
        },
        {
          "key": "d",
          "name": "Start debugging",
          "type": "command",
          "command": "workbench.action.debug.start",
        },
        {
          "key": "c",
          "name": "Continue...",
          "type": "command",
          "command": "workbench.action.debug.continue",
        },
        {
          "key": "i",
          "name": "Step into",
          "type": "command",
          "command": "workbench.action.debug.stepInto",
        },
        {
          "key": "o",
          "name": "Step Out",
          "type": "command",
          "command": "workbench.action.debug.stepOut",
        },
        {
          "key": "v",
          "name": "Step Over",
          "type": "command",
          "command": "workbench.action.debug.stepOver",
        },
        {
          "key": "r",
          "name": "Restart Debugging",
          "type": "command",
          "command": "workbench.action.debug.restart",
        },
        {
          "key": "s",
          "name": "Stop Debugging",
          "type": "command",
          "command": "workbench.action.debug.stop",
        },
      ]
    }
  ]
}
